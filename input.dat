
# PYTHONPATH must include directory above plugin directory.
#     Define either externally or here, then import plugin.
sys.path.insert(0, './..')
import zora_core_excitation

molecule {
O
H 1 R
H 1 R 2 A

R = .9
A = 104.5
}

basis STO-3G

set zora_core_excitation print 0

h_energy = energy('zora_core_excitation')

D_e = psi_hartree2kcalmol * h_energy
print ("E (Hartrees) = %f" % h_energy)
print ("E (kcal/mol) = %f\n" % D_e)